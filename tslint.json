{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "no-unused-expression": false,
    "arrow-parens": ["ban-single-arg-parens"],
    "quotemark": [
      true,
      "single"
    ],
    "indent": [
      true,
      "spaces",
      4
    ]
  },
  "rulesDirectory": []
}
